port: 8080
shards: 5 # amount of room data shards to store data
admin:
  login: admin
  password: changeme
  ips: # (optional) allow access to admin endpoints only from the following IPs
    - 127.0.0.1
cors: # (optional) ref: https://echo.labstack.com/middleware/cors/#configuration
  allow_origins: ['*']
  allow_methods: ['GET', 'POST']
  allow_headers: []
  max_age: 0
path:
  index: testdata/index
  data: testdata/data
servers:
  - etke.cc

